<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
  <Handlers>
    <Console name="Console">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
    </Console>
  </Handlers>
  <Loggers>
    <Root level="error">
      <HandlerRef ref="Console"/>
    </Root>
  </Loggers>
</Configuration>